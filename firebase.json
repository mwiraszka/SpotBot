{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/helloWorld",
        "function": "helloWorld"
      },
      {
        "source": "/login",
        "function": "login"
      },
      {
        "source": "/callback",
        "function": "callback"
      },
      {
        "source": "/refresh_token",
        "function": "refresh_token"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "predeploy": [],
    "source": "functions"
  }
}
